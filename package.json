    "axios": "^0.24.0",
    "lottie-react-native": "^4.1.3",
    "moment": "^2.29.1",
    "react": "16.9.0",
    "react-native": "0.65",
    "react-native-actions-sheet": "^0.5.6",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-loader": "^0.0.9",
