{
  "name": "SiliconPay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "test": "jest",
    "lint": "eslint .",
    "start": "watchman watch-del-all && react-native start -- --reset-cache",
    "clean:andriod": "cd android && ./gradlew clean && cd ../",
    "release:ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle-release": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew bundleRelease -x bundleReleaseJsAndAssets && cd ../",
    "build-debug": "npx react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res && cd android && ./gradlew assembleDebug && cd ../",
    "build-release": "npx react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/build/intermediates/res/merged/release/ && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd ../",
    "gradle:clean": "cd android && gradle --stop && ./gradlew clean build && cd ../"
  },
  "dependencies": {
    "@freakycoder/react-native-bounceable": "^0.2.5",
    "@freakycoder/react-native-empty-state": "^0.1.3",
